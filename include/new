/**
 * Placement new operator.
 * Copyright (C) 2025-present  dbstream
 */
#pragma once

#include <stddef.h>

inline void *
operator new (size_t size, void *ptr)
{
	(void) size;
	return ptr;
}

inline void *
operator new[] (size_t size, void *ptr)
{
	(void) size;
	return ptr;
}

inline void
operator delete (void *ptr1, void *ptr2)
{
	(void) ptr1;
	(void) ptr2;
}

inline void
operator delete[] (void *ptr1, void *ptr2)
{
	(void) ptr1;
	(void) ptr2;
}
